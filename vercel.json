{
    "version": 2,
    "framework": null,
    "installCommand": "npm install",
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "nodeVersion": "20.x"
}